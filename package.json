{
  "name": "Spoggy",
  "version": "0.1.0",
  "contributors": [
    "Scenaristeur"
  ],
  "license": "BSD-3-Clause",
  "repository": "scenaristeur/spoggyfr",
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--max_old_space_size=4096 polymer serve",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=4096 polymer build --auto-base-path && gulp prpl-server",
    "build:static": "cross-env NODE_OPTIONS=--max_old_space_size=4096 polymer build",
    "build:static_es5": "export NODE_OPTIONS=--max_old_space_size=8192 && polymer build --preset es5-bundled",
    "build:static_es5_windows": "set NODE_OPTIONS=--max_old_space_size=4096 && polymer build --preset es5-bundled",
    "build:static_es5_v": "polymer build --preset es5-bundled -v",
    "serve": "prpl-server --root server/build",
    "heroku": "export NODE_OPTIONS=--max_old_space_size=4096 && polymer build && prpl-server --root build/ --host 0.0.0.0 --port $PORT --bot-proxy https://render-tron.appspot.com/render",
    "heroku_local_linux ": "export NODE_OPTIONS=--max_old_space_size=4096 && polymer build && prpl-server --root build/ --host 0.0.0.0",
    "heroku_local_windows": "set NODE_OPTIONS=--max_old_space_size=4096 && polymer build && prpl-server --root build/ --host 0.0.0.0",
    "serve:static": "polymer serve --port 5000 build/es5-bundled"
  },
  "dependencies": {
    "@granite-elements/ace-widget": "^2.2.7-b1",
    "@polymer/app-layout": "^3.0.0",
    "@polymer/iron-collapse": "^3.0.1",
    "@polymer/iron-flex-layout": "^3.0.1",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-dialog": "^3.0.1",
    "@polymer/paper-icon-button": "^3.0.2",
    "@polymer/paper-input": "^3.0.2",
    "@polymer/paper-item": "^3.0.1",
    "@polymer/paper-toggle-button": "^3.0.1",
    "@polymer/polymer": "^3.1.0",
    "@vaadin/vaadin-split-layout": "^4.1.0",
    "@webcomponents/webcomponentsjs": "^2.2.8",
    "browserify": "^16.2.3",
    "evejs": "github:scenaristeur/evejs",
    "lit-element": "^2.0.1",
    "pwa-helpers": "^0.9.1",
    "rdflib": "^0.20.1",
    "solid-auth-client": "^2.3.0",
    "solid-file-client": "^0.5.1",
    "vis": "^4.21.0"
  },
  "devDependencies": {
    "@polymer/test-fixture": "^4.0.2",
    "cross-env": "^5.2.0",
    "polymer-cli": "^1.9.9",
    "polyserve": "^0.27.0"
  }
}
